MAIL_PORT=null #your email port #email portunuz (587=tls(default) / 465=ssl / 25=sendmail / 2525=mailgun / 587=ses / 2525=sparkpost)
MAIL_USERNAME=null #your email address #email adresiniz
MAIL_PASSWORD=null #your email password #email şifreniz
MAIL_ENCRYPTION=null #your email encryption #email şifreleme (ssl / tls)
MAIL_FROM_ADDRESS="contact@splax.com.tr" #your email address #email adresiniz
MAIL_FROM_NAME="Splax" #your name #adınız

AWS_ACCESS_KEY_ID= #your aws access key id #aws erişim anahtarınız
AWS_SECRET_ACCESS_KEY= #your aws secret access key #aws erişim şifreniz
AWS_DEFAULT_REGION=us-east-1 #your aws default region #aws varsayılan bölgeniz
AWS_BUCKET= #your aws bucket #aws bucket adınız
AWS_USE_PATH_STYLE_ENDPOINT=false #your aws use path style endpoint #aws path style endpoint kullanımınız

PUSHER_APP_ID= #your pusher app id #pusher uygulama id'niz
PUSHER_APP_KEY= #your pusher app key #pusher uygulama anahtarınız
PUSHER_APP_SECRET= #your pusher app secret #pusher uygulama şifreniz
PUSHER_HOST= #your pusher host (api.pusherapp.com / api-eu.pusher.com / api-us2.pusher.com / api-ap1.pusher.com)
PUSHER_PORT=443 #your pusher port #pusher portunuz (80 / 443)
PUSHER_SCHEME=https #your pusher scheme #pusher şemalarınız (http / https)
PUSHER_APP_CLUSTER=mt1 #your pusher app cluster #pusher uygulama kümeleriniz (mt1 / eu / us2 / ap1)

VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}" #your pusher app key #pusher uygulama anahtarınız
VITE_PUSHER_HOST="${PUSHER_HOST}" #your pusher host #pusher hostunuz
VITE_PUSHER_PORT="${PUSHER_PORT}" #your pusher port #pusher portunuz
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}" #your pusher scheme #pusher şemalarınız
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}" #your pusher app cluster #pusher uygulama kümeleriniz
