DB_USERNAME=root #your database username #veritabanı kullanıcı adınız
DB_PASSWORD= #your database password #veritabanı şifreniz

BROADCAST_DRIVER=log #log / pusher / redis
CACHE_DRIVER=file #file / database / memcached / redis
FILESYSTEM_DISK=local #local / public / s3
QUEUE_CONNECTION=sync #sync / database / beanstalkd / sqs / redis
SESSION_DRIVER=file #file / cookie / database / memcached / redis / array
SESSION_LIFETIME=120 #your session lifetime #oturum süreniz

MEMCACHED_HOST=127.0.0.1 #your memcached host #memcached hostunuz

REDIS_HOST=127.0.0.1 #your redis host #redis hostunuz
REDIS_PASSWORD=null #your redis password #redis şifreniz
REDIS_PORT=6379 #your redis port #redis portunuz

MAIL_MAILER=smtp #smtp / sendmail / mailgun / ses / sparkpost / log / array
