APP_NAME=api.splax.com.tr #your app name #uygulama adınız
APP_ENV=local #local / production / staging / testing / development / qa / dev 
APP_KEY=base64:zcIwZ7UzKXsof+F7gchBnzzu2AF1lyLwaZkfbGy+L6M= #your app key #uygulama anahtarınız
APP_DEBUG=true #true / false
APP_URL=http://yourdomain.com #your domain  #domaininiz

LOG_CHANNEL=stack #stack / single / daily / syslog / errorlog / monolog / custom / null
LOG_DEPRECATIONS_CHANNEL=null #stack / single / daily / syslog / errorlog / monolog / custom / null
LOG_LEVEL=debug #debug / info / notice / warning / error / critical / alert / emergency

DB_CONNECTION=mysql #mysql / pgsql / sqlite / sqlsrv
DB_HOST=127.0.0.1 #your database host #veritabanı hostunuz
DB_PORT=3306 #your database port #veritabanı portunuz
